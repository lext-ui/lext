{
  "name": "@ynssenem/lext",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "lib/*",
    "example/*"
  ],
  "scripts": {
    "build": "lerna run build --scope @ynssenem/lext",
    "add": "lerna add @ynssenem/lext --scope @ynssenem/example",
    "start": "lerna run --scope @ynssenem/example",
    "watch": "nodemon --watch lib/**/* -e ts,tsx --exec 'npm run build && npm run add'"
  },
  "devDependencies": {
    "lerna": "^8.0.0",
    "nodemon": "^3.0.1"
  }
}
